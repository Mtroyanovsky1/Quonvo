{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "no-plusplus": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "no-magic-numbers": ["error", {"ignore": [1,0,-1]}],
    "jsx-a11y/no-static-element-interactions": 0,
    "import/prefer-default-export": 0
  },
  "globals": {
    "DOMAIN": true
  }
}
